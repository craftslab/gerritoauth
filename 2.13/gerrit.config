[auth]
    gitBasicAuth = true
    type = OAUTH
[plugins]
    allowRemoteAdmin = true
[plugin "gerrit-oauth-provider-github-oauth"]
    client-id = "your-github-oauth-app-client-id"
    client-secret = "your-github-oauth-app-client-secret"
[plugin "gerrit-oauth-provider-uac-oauth"]
    client-id = "your-uac-oauth-app-client-id"
    client-secret = "your-uac-oauth-app-client-secret"
    authorize-url = "http://YOUR_UAC_OAUTH_SERVER_IP:8000/oauth/authorize"
    token-url = "http://YOUR_UAC_OAUTH_SERVER_IP:8000/oauth/token"
    resource-url = "http://YOUR_UAC_OAUTH_SERVER_IP:8000/api/user"
